        "mobileconnection": {},
        "open-remote-window": {},
        "telephony": {},
        "wifi-manage": {},
        "phonenumberservice": {},
        "geolocation": {},
        "systemXHR": {}
  },
  "locales": {
    "en-US": {
      "name": "Tilescreen",
      "description": "An tiled homescreen for FirefoxOS"
    },
    "es-ES": {
      "name": "Tilescreen",
      "description": "Un escritorio tipo mosaico para FirefoxOS"
    },
    "es-MX": {
      "name": "Tilescreen",
      "description": "Un escritorio tipo mosaico para FirefoxOS"
    },
    "ca": {
      "name": "Tilescreen",
      "description": "Un escriptori tipus mosaic per a FirefoxOS"
    }
  },
  "default_locale": "en-US",
  "icons": {
    "128":   "/style/icons/tilescreen.png",
    "60":  "/style/icons/tilescreen60.png",
