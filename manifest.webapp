{
  "name": "Tilescreen",
  "description": "An tiled homescreen for FirefoxOS",
  "launch_path": "/index.html",
  "type": "privileged",
  "role": "homescreen",
  "developer": {
    "name": "leandro713",
    "url": "https://github.com/novia713/tilescreen"
  },
  "permissions": {
    "homescreen-webapps-manage":{
        "description": "Required for manage icon apps in homescreen"
